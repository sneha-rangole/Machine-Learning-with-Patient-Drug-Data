   "source": [
    "# Correlation Heatmap\n",
    "\n",
    "print(\"Correlation Heatmap of Numerical Features:\")\n",
    "print()\n",
    "\n",
    "# Select only numerical columns for correlation (text can't be correlated yet)\n",
    "num_df = df[['Age', 'Dosage_mg', 'Treatment_Duration_days', 'Improvement_Score']]\n",
    "\n",
    "# .corr() computes the Pearson correlation coefficient between every pair of columns\n",
    "corr_matrix = num_df.corr()\n",
    "\n",
    "fig, ax = plt.subplots(figsize=(8, 6))\n",
    "# annot=True: show the number inside each cell\n",
    "# fmt='.2f': format numbers to 2 decimal places\n",
    "# cmap='coolwarm': blue=negative correlation, red=positive correlation, white=zero\n",
    "sns.heatmap(corr_matrix, annot=True, fmt='.2f', cmap='coolwarm', \n",
    "            center=0, ax=ax, square=True, linewidths=1,\n",
    "            cbar_kws={'shrink': 0.8})\n",
    "\n",
    "ax.set_title('Correlation Matrix\\n(Blue = Negative, Red = Positive, White = No relation)', \n",
    "             fontsize=13, fontweight='bold')\n",
    "plt.tight_layout()\n",
    "plt.savefig('correlation_heatmap.png', dpi=150, bbox_inches='tight')\n",
    "plt.show()\n",
    "\n",
    "print(\"HOW TO READ THE HEATMAP:\")\n",
    "print(\"   - Look at the BOTTOM ROW or RIGHT COLUMN (Improvement_Score)\")\n",
    "print(\"   - Red values: feature positively correlates with better improvement\")\n",
    "print(\"   - Blue values: feature negatively correlates with improvement\")\n",
    "print(\"   - Values close to 0: feature has little linear relationship with target\")\n",
    "print()\n",
    "print(\"   Improvement_Score correlations with other features:\")\n",
    "for col in ['Age', 'Dosage_mg', 'Treatment_Duration_days']:\n",
    "    corr = corr_matrix.loc[col, 'Improvement_Score']\n",
    "    direction = \"↑ Positive\" if corr > 0 else \"↓ Negative\"\n",
    "    strength = \"Strong\" if abs(corr) > 0.5 else (\"Moderate\" if abs(corr) > 0.3 else \"Weak\")\n",
    "    print(f\"   {col:30s}: {corr:.3f}  → {direction}, {strength}\")\n"
   ]
  },
