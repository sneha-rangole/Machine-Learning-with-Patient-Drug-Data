    "\n",
    "original_size = len(df)\n",
    "print(f\"Dataset size BEFORE deduplication: {original_size} rows\")\n",
    "\n",
    "# drop_duplicates() removes any row that is 100% identical to a previous row\n",
    "# keep='first' → keep the first occurrence, remove subsequent ones\n",
    "df_clean = df.drop_duplicates(keep='first')\n",
    "\n",
    "removed = original_size - len(df_clean)\n",
    "print(f\"Dataset size AFTER deduplication:  {len(df_clean)} rows\")\n",
    "print(f\"Duplicates removed: {removed}\")\n",
    "\n",
    "if removed == 0:\n",
    "    print(\"No exact duplicates found — dataset is clean on this front.\")\n",
    "else:\n",
    "    print(f\"  {removed} duplicate rows removed!\")\n",
    "    \n",
    "print()\n",
    "print(\"💡 NOTE: We also need to drop Patient_ID — it's an identifier, not a feature.\")\n",
    "print(\"   We'll do this in the encoding step below.\")\n",
    "\n",
    "# Work with df_clean from here on\n",
    "df = df_clean.copy()\n",
    "print(f\"\\n Working dataset confirmed: {df.shape[0]} patients × {df.shape[1]} columns\")"
   ]
