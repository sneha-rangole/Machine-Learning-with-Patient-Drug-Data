    "# Handle Missing Values\n",
    "\n",
    "print(\"🔍 Missing value check before handling:\")\n",
    "print(df.isnull().sum())\n",
    "print()\n",
    "\n",
    "# Our dataset appears complete, but let's demonstrate the proper strategy\n",
    "# In REAL projects, you'd do this:\n",
    "\n",
    "# --- Strategy for NUMERICAL columns ---\n",
    "# Fill with MEDIAN (more robust than mean for skewed data)\n",
    "# median is the middle value — not pulled by extreme outliers like the mean is\n",
    "numerical_cols = ['Age', 'Dosage_mg', 'Treatment_Duration_days']\n",
    "for col in numerical_cols:\n",
    "    n_missing = df[col].isnull().sum()\n",
    "    if n_missing > 0:\n",
    "        median_val = df[col].median()\n",
    "        df[col] = df[col].fillna(median_val)\n",
    "        print(f\"✅ Filled {n_missing} missing values in '{col}' with median ({median_val})\")\n",
    "\n",
    "# --- Strategy for CATEGORICAL columns ---\n",
    "# Fill with MODE (most frequent value)\n",
    "categorical_cols = ['Gender', 'Condition', 'Drug_Name', 'Side_Effects']\n",
    "for col in categorical_cols:\n",
    "    n_missing = df[col].isnull().sum()\n",
    "    if n_missing > 0:\n",
    "        mode_val = df[col].mode()[0]  # mode() returns a Series; [0] gets the top value\n",
    "        df[col] = df[col].fillna(mode_val)\n",
    "        print(f\"✅ Filled {n_missing} missing values in '{col}' with mode ('{mode_val}')\")\n",
    "\n",
    "# Final check\n",
    "remaining = df.isnull().sum().sum()\n",
    "print(f\"\\n{'✅ All missing values handled!' if remaining == 0 else f'⚠️ {remaining} values still missing'}\")\n",
    "print(f\"Dataset shape: {df.shape}\")"
   ]
  },
