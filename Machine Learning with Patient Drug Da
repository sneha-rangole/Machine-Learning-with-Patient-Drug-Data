    "# If left in, the model might accidentally learn \"Patient P0045 always scores 9.6\"\n",
    "# — which is memorization, not generalization.\n",
    "\n",
    "print(\"Removing Patient_ID (identifier column — no predictive value)...\")\n",
    "df = df.drop(columns=['Patient_ID'])\n",
    "print(f\"Dropped Patient_ID. New shape: {df.shape}\")\n",
    "print(f\"   Remaining columns: {list(df.columns)}\")"
   ]
