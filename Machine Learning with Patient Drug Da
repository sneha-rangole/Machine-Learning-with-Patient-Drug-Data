   "source": [
    "# Check Missing Values\n",
    "\n",
    "print(\"Missing Value Analysis:\")\n",
    "print(\"=\" * 70)\n",
    "\n",
    "# isnull() returns True/False for each cell\n",
    "# sum() counts the True values (True = 1, False = 0 in Python)\n",
    "missing = df.isnull().sum()\n",
    "\n",
    "# Calculate percentage missing for each column\n",
    "missing_pct = (missing / len(df) * 100).round(2)\n",
    "\n",
    "# Combine into a readable table\n",
    "missing_df = pd.DataFrame({\n",
    "    'Missing Count': missing,\n",
    "    'Missing %': missing_pct,\n",
    "    'Status': [' Has missing values' if v > 0 else '✅ Complete' for v in missing]\n",
    "})\n",
    "\n",
    "print(missing_df)\n",
    "print()\n",
    "\n",
    "total_missing = missing.sum()\n",
    "if total_missing == 0:\n",
    "    print(\" GREAT NEWS: No missing values found in this dataset!\")\n",
    "    print(\"   In real-world data, you'd typically have many missing values.\")\n",
    "    print(\"   We'll still show you how to handle them as a learning exercise.\")\n",
    "else:\n",
    "    print(f\"  Total missing cells: {total_missing}\")\n",
    "    print(\"   We need to handle these before training — see Phase 2B.\")"
   ]
  },
